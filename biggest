a=int(input('enter a:'))
b=int(input('enter b:'))
c=int(input('enter c:'))
d=int(input('enter d:'))
if a>b:
    if a>c:
        if a>d:
            print('a is big')
if b>a:
    if b>c:
        if b>d:
            print('b is big')
if c>a:
    if c>b:
        if c>d:
            print('c is big')
else :
    print('d is big')
